<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:uer="using:Uno.Extensions.Reactive.UI"
					xmlns:winui="using:Microsoft.UI.Xaml.Controls"
					xmlns:mobile="http://platform.uno/mobile"
					xmlns:not_skia="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
					xmlns:utu="using:Uno.Toolkit.UI"
					mc:Ignorable="mobile">

	<mobile:Style x:Key="ChefsMainCommandStyle"
				  BasedOn="{StaticResource MaterialMainCommandStyle}"
				  TargetType="AppBarButton">
		<Setter Property="Content" Value="" />
	</mobile:Style>

	<mobile:Style x:Key="ChefsNavigationBarStyle"
	TargetType="utu:NavigationBar">
		<Setter Property="MainCommandStyle" Value="{StaticResource ChefsMainCommandStyle}" />
	</mobile:Style>

	<mobile:Style BasedOn="{StaticResource ChefsNavigationBarStyle}"
	TargetType="utu:NavigationBar" />
</ResourceDictionary>
